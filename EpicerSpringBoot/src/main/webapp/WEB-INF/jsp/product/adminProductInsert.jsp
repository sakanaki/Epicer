<script>
function insert() {
	Swal.fire({
		  icon: 'success',
		  title: '新增完成',
		  showConfirmButton: false,
		  timer: 1000
		  
		})
		
		.then((result) => {
	  
	    $("#updateProduct").submit();
	  
	})
}
</script>
